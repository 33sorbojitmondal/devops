{
  "name": "devops-todo-frontend",
  "version": "1.0.0",
  "description": "Frontend for DevOps learning Todo application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
